(d / deny-01
  :ARG0 (c / country
          :name (n / name
                  :op1 "North"
                  :op2 "Korea"))
  :ARG1 (a / access-01
          :ARG0 o
          :ARG1 (f / facility
                  :poss c)
          :mod (f2 / full))
  :ARG2 (o / organization
          :name (n2 / name
                  :op1 "IAEA"))) 
